public class Animal {
    private String type;
    private String Size;
    private double Weight;


    public Animal(){

    }
    public Animal(String typ,String sze,double wt){
        this.type = typ;
        this.Size = sze;
        this.Weight = wt;
    }
    // lets keep this code simple,so we will leave the getters and setters
    // but we do want a method to print the fields out so use to String Methods

    @Override
    public String toString() {
        return "Animal{" +
                "type='" + type + '\'' +
                ", Size='" + Size + '\'' +
                ", Weight=" + Weight +
                '}';
    }
    public void move(String speed){
        System.out.println(type + " moves " + speed);
    }
    public void makeNoise(){
        System.out.println(type + " makes some kind of noise ");
    }
}
